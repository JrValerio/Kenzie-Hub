{
  "name": "kenzie-hub-api",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "db:wait": "node scripts/wait-for-db.js",
    "start:prod": "npm run db:wait && npm run start",
    "postinstall": "prisma generate",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "npm run db:wait && prisma migrate deploy",
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.4",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.19.2",
    "wait-on": "^9.0.4"
  }
}
